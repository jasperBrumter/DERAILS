<%= simple_form_for [@ticket] do |f| %>
  <%= f.input :city %>
  <%= f.input :postal_code %>
  <%= f.input :status %>
  <%= f.input :photo_1 %>
  <%= f.input :photo_1_cache, as: :hidden %>
  <%= f.input :photo_2 %>
  <%= f.input :photo_2_cache, as: :hidden %>
  <%= f.input :photo_3 %>
  <%= f.input :photo_3_cache, as: :hidden %>
  <%= f.input :description %>
  <%= f.input :tune_up %>
  <%= f.input :brakes %>
  <%= f.input :flat_tire %>
  <%= f.input :chain %>
  <%= f.input :gears %>
  <%= f.input :wheels %>
  <%= f.input :frame %>
  <%= f.input :other %>
  <%= f.submit %>
<% end %>





